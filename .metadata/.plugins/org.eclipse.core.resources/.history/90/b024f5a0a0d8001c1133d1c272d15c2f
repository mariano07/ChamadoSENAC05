package paco;

import javax.swing.JOptionPane;

public class Defeito {
	
	String problema;
	
	public void Imprime() {
		
		JOptionPane.showMessageDialog(null,"teste01");
		
	}
	
	public void QualDef() {
		
				
		problema = JOptionPane.showInputDialog("Qual problema?");
	}
	
	public void ImprimeLaudo() {
		
		JOptionPane.showMessageDialog(null, problema);
	}
	
}